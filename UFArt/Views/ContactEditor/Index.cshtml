@model ContactViewModel

<div class="p-3">
    <form method="post" asp-controller="ContactEditor" asp-action="UploadAsync">
        <div class="form-group">
            <label asp-for="Email">Adres email</label>
            <input asp-for="Email" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Telephone">Numer telefonu</label>
            <input asp-for="Telephone" classs="form-control" />
        </div>
        <div class="text-center">
            <input type="submit" class="btn btn-primary" value="Zatwierdź" />
        </div>
    </form>
</div>