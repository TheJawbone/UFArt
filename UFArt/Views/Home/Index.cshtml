@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light" rel="stylesheet">
    <link rel="stylesheet" asp-href-include="~/css/vendor.min.css" />
    <link rel="stylesheet" asp-href-include="~/css/style.css" />
    <link rel="stylesheet" asp-href-include="~/css/dropdown.css" />
    <title>Urszula Figiel Art</title>
</head>
<body class="bg-light h-100">
    <div id="carouselExampleControls" class="carousel slide h-100" data-interval="10000" data-ride="carousel" style="background-size: cover;">
        <div class="carousel-inner h-100" generate-slides></div>
        <div class="d-opaque-white-bg h-100"></div>
        <div class="entry-text">
            <h1>Urszula Figiel <span class="text-primary">Art</span></h1>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <a href="/About" class="btn btn-primary entry-button">Przejdź do strony</a>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/vendor.min.js"></script>
    <script src="~/js/dropdown.js"></script>
</body>
</html>